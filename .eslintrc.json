{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "plugins": [
    "babel",
    "react"
  ],
  "rules": {
    "babel/new-cap": 1,
    "babel/no-invalid-this": 0,
    "babel/object-curly-spacing": 0,
    "babel/quotes": 0,
    "babel/semi": 1,
    "babel/no-unused-expressions": 1,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "class-methods-use-this": 0,
    "no-restricted-syntax": 0,
    "arrow-parens": 0,
    "no-plusplus": 0,
    "no-console": 0,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ]
  }
}